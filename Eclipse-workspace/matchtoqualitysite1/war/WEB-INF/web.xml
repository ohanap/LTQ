<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.matchtoquality.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/matchtoqualitysite/greet</url-pattern>
  </servlet-mapping>
  

  
  <servlet>         
  	<servlet-name>SendMail</servlet-name>         
  	<servlet-class>com.matchtoquality.MailServlet</servlet-class>     
  </servlet>     
  <servlet-mapping>         
  	<servlet-name>SendMail</servlet-name>         
  	<url-pattern>/SendMail</url-pattern>     
  </servlet-mapping> 
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
